{
  "name": "basic-electron-react-boilerplate",
  "version": "0.7.0",
  "description": "Minimal and modern react+electron+webpack boilerplate",
  "author": "Phillip Barbiero",
  "homepage": "https://github.com/pbarbiero/basic-electron-react-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbarbiero/basic-electron-react-boilerplate.git"
  },
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prod": "webpack --config webpack.build.config.js --mode production && electron --noDevServer .",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "webpack --config webpack.build.config.js --mode production",
    "package": "webpack --config webpack.build.config.js --mode production",
    "postpackage": "electron-packager ./ --out=./builds"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "tailwindcss": "^1.1.2",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^2.1.0",
    "electron": "^6.0.4",
    "electron-packager": "^13.0.1",
    "extract-text-webpack-plugin": "~4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-dropzone": "^10.1.7",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.39.2",
    "webpack-cli": "~3.2.3",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "clean-css": "^4.2.1",
    "core-js": "^3.2.1",
    "esm": "^3.2.25",
    "file-type": "^12.2.0",
    "fs-extra": "^8.1.0",
    "handbrake-js": "^4.0.0",
    "html-minifier": "^4.0.0",
    "imagemin": "^7.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "jimp": "^0.6.4",
    "jszip": "^3.2.2",
    "mime": "^2.4.4",
    "p-queue": "^6.1.1",
    "terser": "^4.2.1"
  }
}
