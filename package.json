{
  "name": "basic-electron-react-boilerplate",
  "version": "0.7.0",
  "description": "Minimal and modern react+electron+webpack boilerplate",
  "author": "Phillip Barbiero",
  "homepage": "https://github.com/pbarbiero/basic-electron-react-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbarbiero/basic-electron-react-boilerplate.git"
  },
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prod": "webpack --config webpack.build.config.js --mode production && electron --noDevServer .",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "webpack --config webpack.build.config.js --mode production",
    "package": "webpack --config webpack.build.config.js --mode production",
    "postpackage": "electron-packager ./ --out=./builds"
  },
  "devDependencies": {
    "@babel/core": "*",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@fullhuman/postcss-purgecss": "*",
    "autoprefixer": "*",
    "babel-loader": "^8.0.6",
    "babili-webpack-plugin": "*",
    "css-loader": "^3.2.0",
    "core-js": "*",
    "electron": "^6.0.7",
    "electron-packager": "^14.0.5",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "*",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "*",
    "react": "*",
    "react-dom": "*",
    "react-dropzone": "^10.1.8",
    "style-loader": "^1.0.0",
    "tailwindcss": "*",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "*"
  },
  "dependencies": {
    "clean-css": "*",
    "esm": "*",
    "fs-extra": "*",
    "handbrake-js": "*",
    "html-minifier": "*",
    "html5-file-selector": "^2.1.0",
    "imagemin": "*",
    "imagemin-gifsicle": "*",
    "imagemin-mozjpeg": "*",
    "imagemin-pngquant": "*",
    "imagemin-svgo": "*",
    "jimp": "^0.6.7",
    "jszip": "*",
    "mime": "*",
    "p-queue": "*",
    "terser": "*"
  }
}
